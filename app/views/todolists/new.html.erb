	<div class="new">
	    <h1 class="new-head">新規投稿</h1>

		<%= form_for(@list, url: '/todolists') do |f| %>

		<h4 class="new-content">タイトル</h4>
		<%= f.text_field :title %>

		<h4 class="new-content">本文</h4>
		<%= f.text_area :body %>

		<h4 class="new-content">画像</h4>
		<%= f.attachment_field :image %>

	<%= f.submit '投稿' %>
</div>
<% end %>